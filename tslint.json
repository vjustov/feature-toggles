{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case",
        "require-const-for-all-caps"
      ]
    }
  }
}
